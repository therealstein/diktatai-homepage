---
import Layout from '../../layouts/Layout.astro';
import LegalPage from '../../components/LegalPage.astro';
import { legalTranslations } from '../../i18n/legal';
import { privacyContent } from '../../i18n/legal-content/privacy';

const locale = 'nl';
const t = legalTranslations.privacy[locale];
---

<Layout
  title={t.meta.title}
  description={t.meta.description}
  locale={locale}
  routeKey="privacy"
  noindex={true}
>
  <LegalPage title={t.title} content={privacyContent[locale]} />
</Layout>
