---
import Layout from '../../layouts/Layout.astro';
import PublicSectorPage from '../../components/PublicSectorPage.astro';
import { publicSectorTranslations } from '../../i18n/userGroups/publicSector';

const locale = 'en';
const t = publicSectorTranslations[locale];
---

<Layout
  title={t.hero.title}
  description={t.seo.description}
  locale={locale}
  routeKey="publicSector"
>
  <PublicSectorPage locale={locale} t={t} />
</Layout>
