---
import Layout from '../layouts/Layout.astro';
import MediaPage from '../components/MediaPage.astro';
import { mediaTranslations } from '../i18n/userGroups/media';

const locale = 'de';
const t = mediaTranslations[locale];
---

<Layout
  title={t.seo.title}
  description={t.seo.description}
  locale={locale}
  routeKey="media"
>
  <MediaPage locale={locale} t={t} />
</Layout>
