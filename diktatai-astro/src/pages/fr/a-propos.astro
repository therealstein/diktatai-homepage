---
import Layout from '../../layouts/Layout.astro';
import AboutPage from '../../components/AboutPage.astro';
import { aboutTranslations } from '../../i18n/userGroups/about';

const locale = 'fr';
const t = aboutTranslations[locale];
---

<Layout
  title={t.meta.title}
  description={t.meta.description}
  locale={locale}
  routeKey="about"
>
  <AboutPage locale={locale} t={t} />
</Layout>
