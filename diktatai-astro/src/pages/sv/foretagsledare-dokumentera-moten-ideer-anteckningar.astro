---
import Layout from '../../layouts/Layout.astro';
import SalesPage from '../../components/SalesPage.astro';
import { salesTranslations } from '../../i18n/userGroups/sales';

const locale = 'sv';
const t = salesTranslations[locale];
---

<Layout
  title={t.seo.title}
  description={t.seo.description}
  locale={locale}
  routeKey="sales"
>
  <SalesPage locale={locale} t={t} />
</Layout>
